&sdhci0 {
   status = "okay";
   bus-width = <8>;
   max-frequency = <50000000>;
};

/* SD1 with level shifter */
&sdhci1 {
   status = "okay";
   disable-wp;
   max-frequency = <50000000>;
   no-1-8-v;   /* for 1.0 silicon */
};

