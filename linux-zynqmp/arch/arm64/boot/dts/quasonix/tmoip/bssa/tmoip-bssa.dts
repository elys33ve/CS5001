/ {
	chosen {
		bootargs = "earlycon console=ttyPS0,115200 clk_ignore_unused root=/dev/mmcblk1p2 rw rootwait";
		stdout-path = "serial0:115200n8";
	};
	aliases {
		ethernet0 = &gem3;
		ethernet1 = &gem0;
		serial0 = &uart0;
		serial1 = &uart1;
	};
	memory {
		device_type = "memory";
		reg = <0x0 0x0 0x0 0x7ff00000>, <0x00000008 0x00000000 0x0 0x80000000>;
	};

  	reserved-driver@0 {
      	compatible = "xlnx,reserved-memory";
		memory-region = <&reserved_fb>;
   	};	
   	reserved-memory {
        #address-cells = <2>;
        #size-cells = <2>;
		ranges;
  	   reserved_fb: buffer@0 {
           no-map;
		   reg = <0x0 0x10000000 0x0 0x4000000>;
      		   };
   	};

    framebuffer0: framebuffer@10000000 {
         compatible = "simple-framebuffer";
         #address-cells = <2>;
         #size-cells = <2>;
	 reg = <0x0 0x10000000 0x0 (160 * 128 * 2)>;
	 width = <160>;
	 height = <128>;
	 stride = < (160 * 2) >;
	 format = "r5g6b5";
	};
    framebuffer1: framebuffer@11000000 {
         compatible = "simple-framebuffer";
         #address-cells = <2>;
         #size-cells = <2>;
	 reg = <0x0 0x11000000 0x0 (160 * 128 * 2)>;
